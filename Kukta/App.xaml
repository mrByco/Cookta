<Application
    x:Class="Kukta.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Kukta"
    RequestedTheme="Light">
    <Application.Resources>
        <SolidColorBrush x:Key="Gray0" Color="#1e1e1e"/>
        <SolidColorBrush x:Key="Gray1" Color="#333333"/>
        <SolidColorBrush x:Key="Gray2" Color="#444444"/>
        <SolidColorBrush x:Key="Gray3" Color="#555555"/>
        <SolidColorBrush x:Key="Gray4" Color="#666666"/>
        <SolidColorBrush x:Key="Gray5" Color="#777777"/>
        <SolidColorBrush x:Key="Gray6" Color="#888888"/>
        <SolidColorBrush x:Key="Gray7" Color="#191919"/>
    </Application.Resources>

</Application>
